# MAA自动化系统环境变量配置
# 请将此文件复制为 .env 并填入您的实际配置信息

# WebHook通知配置 (Server酱3)
WEBHOOK_UID=your_uid_here
WEBHOOK_TOKEN=your_token_here
WEBHOOK_BASE_URL=https://[uid].push.ft07.com/send/[token].send

# ADB设备配置
ADB_DEVICE=localhost:35555

# MAA安装路径
MAA_BINARY_PATH=/root/.cargo/bin/maa
MAA_PYTHON_PATH=/Task/MAA/Python
UV_PYTHON_PATH=/root/.local/bin/uv

# BAAH配置
BAAH_PATH=/Task/BAAH
BAAH_TASK_FILE=task.json

# 数据库配置（未来扩展使用）
# DATABASE_URL=sqlite:///maa_auto.db

# 安全配置
WEB_SECRET_KEY=your_secret_key_here_change_this_in_production

# 调试配置
DEBUG_MODE=false
LOG_LEVEL=INFO